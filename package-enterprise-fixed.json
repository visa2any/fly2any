{
  "name": "fly2any",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "node start-server.js",
    "dev:direct": "next dev",
    "dev:fallback": "node run-dev-fixed.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test:env": "node test-environment.js",
    "auth:generate-secret": "openssl rand -base64 32",
    "auth:check": "node -e \"console.log(require('./auth.ts'))\"",
    "css:build": "tailwindcss -i ./src/app/globals.css -o ./dist/output.css",
    "css:watch": "tailwindcss -i ./src/app/globals.css -o ./dist/output.css --watch",
    "network:test": "node network-diagnostic.js",
    "ux:test": "node run-ux-tests.js",
    "ux:test:visual": "npx playwright test tests/ux/flight-search-glassmorphism.spec.js --config=playwright.ux.config.js",
    "ux:test:performance": "npx playwright test tests/ux/performance-glassmorphism.spec.js --config=playwright.ux.config.js",
    "ux:test:accessibility": "npx playwright test tests/ux/accessibility-glassmorphism.spec.js --config=playwright.ux.config.js",
    "ux:test:headed": "npx playwright test tests/ux/ --config=playwright.ux.config.js --headed",
    "ux:test:debug": "npx playwright test tests/ux/ --config=playwright.ux.config.js --debug",
    "ux:report": "npx playwright show-report test-results/ux-comprehensive-report",
    "ux:install": "npx playwright install chromium firefox webkit",
    "ux:demo": "node ux-test-demo.js"
  },
