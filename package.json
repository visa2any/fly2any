    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=8192' next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--max-old-space-size=8192 --inspect' next dev --turbo",
    "dev:npx": "npx next dev",
    "dev:win": "node node_modules\\next\\dist\\bin\\next dev",
    "build": "next build",
    "build:optimized": "node scripts/build-optimized.js",
    "build:analyze": "ANALYZE=true next build",
    "start": "node node_modules/next/dist/bin/next start",
    "lint": "next lint --cache",
    "lint:fix": "next lint --cache --fix",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --incremental --watch",
    "clean": "rm -rf .next && rm -rf .swc",
    "clean:cache": "rm -rf .next/cache && rm -rf node_modules/.cache",
    "cache:setup": "node scripts/cache-optimizer.js setup",
    "cache:clear": "node scripts/cache-optimizer.js clear",
    "cache:optimize": "node scripts/cache-optimizer.js optimize",
    "cache:analyze": "node scripts/cache-optimizer.js analyze",
    "cache:preoptimize": "node scripts/cache-optimizer.js preoptimize",
    "performance:dev": "node scripts/dev-performance.js",
    "performance:measure": "node scripts/measure-performance.js",
    "test": "npx playwright test",
    "test:auth": "node tests/run-auth-tests.js",
    "test:auth:quick": "node tests/run-auth-tests.js quick",
    "test:auth:csrf": "node tests/run-auth-tests.js csrf",
    "test:auth:performance": "node tests/run-auth-tests.js performance",
    "test:auth:visual": "node tests/run-auth-tests.js visual",
    "test:auth:monitoring": "node tests/run-auth-tests.js monitoring",
    "test:auth:network": "node tests/run-auth-tests.js network",
    "test:auth:report": "npx playwright show-report tests/reports/html",
    "test:mobile": "node run-mobile-diagnostic.js",
    "test:mobile:quick": "node mobile-form-click-test.js",
    "test:mobile:full": "node mobile-leadcapture-diagnostic-test.js",
    "test:cross-device": "node cross-device-compatibility-test.js",
    "test:cross-device:quick": "node test-cross-device-quick.js",
    "test:cross-device:runner": "node run-cross-device-test.js"
