{
  "name": "fly2any",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "admin:create": "node scripts/create-admin.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:all": "npm run test && npm run test:e2e",
    "test:fares": "playwright test tests/e2e/fare-reconciliation/",
    "test:fares:api": "playwright test tests/e2e/fare-reconciliation/api-tests.spec.ts",
    "test:fares:report": "npx ts-node tests/e2e/fare-reconciliation/report-generator.ts",
    "monitor:fares": "npx ts-node scripts/fare-monitor.ts",
    "monitor:fares:continuous": "npx ts-node scripts/fare-monitor.ts --continuous",
    "monitor:uptime": "ts-node scripts/uptime-monitor.ts",
    "monitor:uptime:continuous": "ts-node scripts/uptime-monitor.ts --continuous",
    "mobile:build": "node scripts/mobile-build.js",
    "mobile:sync": "npm run mobile:build && npx cap sync",
    "mobile:ios": "npm run mobile:sync && npx cap open ios",
    "mobile:android": "npm run mobile:sync && npx cap open android",
    "mobile:run:ios": "npm run mobile:sync && npx cap run ios",
    "mobile:run:android": "npm run mobile:sync && npx cap run android",
    "mobile:build:ios": "npm run mobile:sync && cd ios/App && xcodebuild -workspace App.xcworkspace -scheme App -configuration Release",
    "mobile:build:android": "npm run mobile:sync && cd android && ./gradlew assembleRelease"
  },
